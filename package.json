{
  "name": "storybook-vue3-router",
  "version": "3.0.0",
  "description": "A Storybook decorator that allows you to build stories for your routing-aware components.",
  "keywords": [
    "storybook-addons",
    "vue",
    "vue-router"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/NickMcBurney/storybook-vue3-router"
  },
  "author": "Nick McBurney",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "types": "dist/ts/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "prebuild": "pnpm clean",
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "storybook": "storybook dev -p 6006",
    "dev": "concurrently \"pnpm storybook\" \"pnpm build:watch\"",
    "build-storybook": "storybook build",
    "release": "pnpm build && auto shipit",
    "test": "npx playwright test",
    "eject-ts": "zx scripts/eject-typescript.mjs"
  },
  "dependencies": {
    "vue-router": "4.x"
  },
  "devDependencies": {
    "@playwright/test": "^1.26.1",
    "@storybook/addon-actions": "^7.0.0-rc.3",
    "@storybook/addon-essentials": "7.0.0-rc.3",
    "@storybook/addons": "^7.0.0-rc.3",
    "@storybook/blocks": "7.0.0-rc.3",
    "@storybook/preview-api": "^7.0.0-rc.3",
    "@storybook/types": "7.0.0-rc.3",
    "@storybook/vue3": "^7.0.0-rc.3",
    "@storybook/vue3-vite": "^7.0.0-rc.3",
    "@types/node": "18",
    "@vitejs/plugin-vue": "^4.0.0",
    "auto": "^10.3.0",
    "concurrently": "^6.2.0",
    "esbuild": "^0.17.11",
    "jest": "^27.4.5",
    "prettier": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.19.1",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "storybook": "7.0.0-rc.3",
    "typescript": "^4.2.4",
    "vite": "^4.1.4",
    "vue": "^3.2.47",
    "zx": "^1.14.1"
  },
  "peerDependencies": {
    "@storybook/vue3": "7.x",
    "vue": "3.x",
    "vue-router": "4.x"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "vue-router": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "Vue 3 Router",
    "supportedFrameworks": [
      "vue"
    ],
    "icon": "https://github.com/NickMcBurney/storybook-vue3-router/blob/main/vue-logo.png?raw=true"
  }
}
